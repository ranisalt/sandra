{
  "name": "sandra",
  "version": "0.1.0",
  "description": "A benchmarking library for modern, promised Javascript.",
  "main": "lib/index.js",
  "jsnext:main": "src/index.js",
  "scripts": {
    "build": "babel -d lib/ src/",
    "clean": "rm -rf coverage .nyc_output",
    "prepublish": "npm run build && npm run test && npm run clean",
    "test": "xo && nyc --reporter=lcov ava -r babel-register"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/ranisalt/sandra.git"
  },
  "keywords": [
    "benchmark"
  ],
  "author": "Ranieri Althoff <ranisalt@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ranisalt/sandra/issues"
  },
  "homepage": "https://github.com/ranisalt/sandra#readme",
  "dependencies": {
    "microtime": "^2.1.1"
  },
  "devDependencies": {
    "ava": "^0.15.2",
    "babel-cli": "^6.11.4",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.11.6",
    "nyc": "^7.1.0"
  },
  "xo": {
    "esnext": true,
    "ignores": "lib/**",
    "rules": {
      "babel/no-await-in-loop": 0,
      "no-unmodified-loop-condition": 0
    },
    "semicolon": false,
    "space": true
  }
}
